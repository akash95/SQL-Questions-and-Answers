CREATE TABLE IF NOT EXISTS date_date
(
  date DATE NOT NULL,
  year INT NOT NULL,
  Quarter varchar(20) NOT NULL,
  Month varchar(20) NOT NULL,
  PRIMARY KEY(DATE)
  );
  
INSERT INTO date_date(date , year, Quarter, Month)
VALUES 
