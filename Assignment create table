CREATE TABLE IF NOT EXISTS student_student
(
student_id uuid,
teacher_id uuid,
city_id uuid,
student_name varchar(20) NOT NULL,
grade varchar(20) NOT NULL,
class_start_date DATE NOT NULL,
PRIMARY KEY(student_id, teacher_id, city_id)
);

